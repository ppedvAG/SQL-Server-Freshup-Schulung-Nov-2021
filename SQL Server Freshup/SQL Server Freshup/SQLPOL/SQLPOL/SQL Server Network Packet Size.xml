                        &lt;TypeClass&gt;Numeric&lt;/TypeClass&gt;&lt;?char 13?&gt;
                        &lt;ObjType&gt;System.Int32&lt;/ObjType&gt;&lt;?char 13?&gt;
                        &lt;Value&gt;8060&lt;/Value&gt;&lt;?char 13?&gt;
                        &lt;/Constant&gt;&lt;?char 13?&gt;
                        &lt;/Operator&gt;
                      </DMF:Expression>
                      <DMF:Name type="string">Optimierte Netzwerkpaketgröße</DMF:Name>
                      <DMF:Description type="string">Bestätigt, dass der Wert, der für die Serveroption der Netzwerkpaketgröße angegeben ist, 8060 Bytes nicht überschreitet.</DMF:Description>
                      <DMF:Facet type="string">IServerPerformanceFacet</DMF:Facet>
